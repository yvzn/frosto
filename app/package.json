{
	"name": "frosto-app",
	"version": "1.0.0",
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check build-only",
		"preview": "vite preview --port 4173",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"postbuild": "purgecss --content dist/*.html dist/assets/*.js --css dist/assets/*.css --output dist/assets/"
	},
	"dependencies": {
		"bootstrap": "^5.2.2",
		"pinia": "^2.0.21",
		"vue": "^3.2.38",
		"vue-router": "^4.1.5"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.1.4",
		"@types/node": "^16.11.56",
		"@vitejs/plugin-vue": "^3.0.3",
		"@vue/eslint-config-prettier": "^7.0.0",
		"@vue/eslint-config-typescript": "^11.0.0",
		"@vue/tsconfig": "^0.1.3",
		"eslint": "^8.22.0",
		"eslint-plugin-vue": "^9.3.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"purgecss": "^5.0.0",
		"typescript": "~4.8.4",
		"vite": "^3.2.8",
		"vue-tsc": "^1.0.7"
	}
}
